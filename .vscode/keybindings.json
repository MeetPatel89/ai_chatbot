// Place your key bindings in this file to override the defaults
[
  // {
  //       "key": "ctrl+i",
  //       "command": "composerMode.agent"
  //   },
  //   {
  //       "key": "ctrl+e",
  //       "command": "composerMode.background"
  //   },
    {
    "key": "ctrl+e",
    "command": "workbench.view.explorer",
    "when": "editorTextFocus || terminalFocus"
  },
  {
    "key": "ctrl+e",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "explorerViewletVisible && filesExplorerFocus"
  },
  {
    "key": "capslock",
    "command": "extension.vim_escape",
    "when": "editorTextFocus && vim.active && (vim.mode == 'Insert' || vim.mode == 'Replace')"
  },
  {
  "key": "ctrl+k",
  "command": "workbench.action.nextEditorInGroup",
  "when": "editorTextFocus && !terminalFocus"
},
{
  "key": "ctrl+j",
  "command": "workbench.action.previousEditorInGroup",
  "when": "editorTextFocus && !terminalFocus"
},
  {
    "key": "ctrl+r",
    "command": "python.execInTerminal",
    "when": "editorTextFocus && editorLangId == 'python'"
  },
  {
  "key": "ctrl+n",
  "command": "workbench.action.terminal.kill",
},
  {
    "key": "ctrl+m",
    "command": "workbench.action.terminal.focus",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+m",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+y",
    "command": "workbench.action.focusActiveEditorGroup"
  },
  {
    "key": "ctrl+N",
    "command": "explorer.newFile",
    "when": "explorerViewletVisible && filesExplorerFocus"
  },
  { "key": "m", "command": "notebook.cell.changeToMarkdown",
    "when": "notebookCellListFocused" },
  { "key": "y", "command": "notebook.cell.changeToCode",
    "when": "notebookCellListFocused" },
    {
  "key": "ctrl+;",
  "command": "workbench.action.closeActiveEditor",
  "when": "editorTextFocus || editorFocus"
}
]
